TypeTesting
-----------
Some type-testing
-----------


cat Monoid
	(+) : monoid -> monoid -> monoid

type List a	= {Empty} + List' a
type List' a	= {Elem head:a; elem:List a}

cat Set (a:Eq)
	> True

instance List a is Monoid

cat Eq
	(==) : eq -> eq -> eq

instance List (a:Eq) is Eq

instance List is Set
