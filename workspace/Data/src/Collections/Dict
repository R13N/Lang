Dict
----
Implements the typeclass for a dictionary and additional functions.
----

import Category.Eq
import Category.Functor
import Category.Monoid

-- A ''Dict'' (dictionary) is any type that maps a certain key onto a certain value.
cat Dict (k:Eq) v
	empty	: dict k v
	insert	: k -> v -> dict k v -> dict k v
	lookup	: k -> dict k v -> v
	head	: dict k v -> (k,v)
	tail	: dict k v -> dict k v

instance Functor (Dict k)

instance Monoid (Dict k v)
