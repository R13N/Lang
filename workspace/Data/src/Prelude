---


data X	= XTrue | XFalse
data Y	= YTrue | YFalse

(&&)		: X -> X -> X
 XTrue XTrue	= XTrue
 *		= XFalse
(&&)		: Y -> Y -> Y
 YTrue YTrue	= YTrue
 *		= YFalse

strue	: Bool
	= True
strue	: X
	= XTrue
